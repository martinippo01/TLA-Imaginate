val grayScale: 20

def myOwnModifier():
    .addContrast(10, 20).addGrayscale(grayScale)

Imaginate
    .forEachFocus("./luna.png","./zapatillas.png","./reloj.png")
    .addBackground("./darkblue.png")
    .addFlavour("./nubes.png")
    .pickFlavour("./estrellas.png", "./truenos.png", "./lluvia.png")
    .addFlavour("./niebla.png")
    .myOwnModifier()
    .renderAll()
